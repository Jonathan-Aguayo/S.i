public class OompaLoompa 
{
    private String name;
    private double weight;
    OompaLoompa(String name,double weight)
    {
        this.name=name;
        this.weight=weight;
    }

    public String getName()
    {
        return name;
    }

    public double getWeight()
    {
        return weight;
    }

    public void eat(int food)
    {
        weight=weight+food;
    }

    public void sleep(int hours)
    {
        weight=weight-hours;;
    }

    public String toString()
    {
        return "Name: "+getName() + "weight: "+ getWeight();
    }

    public int compareTo(Object x)
    {
    
    }

    public static void main(String [] args)
    {
        OompaLoompa jerry=new OompaLoompa("Jerry",100);
        OompaLoompa george=new OompaLoompa("George",110);
        OompaLoompa george2=new OompaLoompa("George",105);

        //Test comparing alphabetically first,then by weight
        System.out.println(jerry.compareTo(george));
        //shoule be 3
        System.out.println(george.compareTo(george2));
        //should be 5

    }
}
