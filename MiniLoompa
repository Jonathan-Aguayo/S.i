
/**
 * Write a description of class MiniLoompa here.
 *
 * @author (your name)
 * @version (a version number or a date)
 */
public class MiniLoompa extends OompaLoompa
{
   

    public static void main(String [] args)
    {
        MiniLoompa terry=new MiniLoompa("Terry",50);
        MiniLoompa mark=new MiniLoompa("Mark",60);

        terry.eat();
        terry.eat();
        terry.eat();
        terry.sleep();

        System.out.println(terry);
        System.out.println("Expected: Terry 60.0");

        mark.eat();
        mark.eat();
        mark.eat();
        mark.eat();
        mark.eat();
        mark.eat();

        System.out.println(mark);
        System.out.println("Expected: Mark 90.0");

    }
}
