
public class Cat
{
    //Instance vars
    private String name;
    private double height;
    private final int MAXIMUM_UNDERWATER_DEPTH=2 ;
    private final double MAXIMUM_TIME_UNDERWATER= 3.4;
    //constructor
    
    public Cat (String Name,double Height)
    {
        this.name = Name;
        this.height=Height;
    }

    //methods
    public String getName()
    {
        return name;
    }

    public void setName(String newName)
    {
        name=newName;
    }

    public int maxDepth()
    {
        return MAXIMUM_UNDERWATER_DEPTH;
    }

    public double maxTime()
    {
        return MAXIMUM_TIME_UNDERWATER;
    }

    public double getMeasure()
    {
        return height;
    }

}
