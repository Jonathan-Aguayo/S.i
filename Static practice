public class StaticTester
{
    public static void main(String [] args)
    {
        String [] secretPassword = {"P0tatoe", "L1ma beans", "Pickl3", "Gr4vy"};

        String password = Utility.codeCracker(secretPassword);

        System.out.println(password);

        System.out.println("Expected: " + "0" + "1" + "3" + "4");

    }
}

