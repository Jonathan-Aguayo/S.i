public class Dog 
{
    //Instance vars
    private String name;
    private double height;
    //depth in meters
    private final int MAXIMUM_UNDERWATER_DEPTH= 4;
    
    private final double MAXIMUM_TIME_UNDERWATER= 1.3;
    //constructor
    public Dog (String Name,double Height)
    {
        this.name = Name;
        this.height=Height;
    }

    //methods
    public String getName()
    {
        return name;
    }

    public void setName(String newName)
    {
        name=newName;
    }
    
    public int maxDepth()
    {
        return MAXIMUM_UNDERWATER_DEPTH;
    }
    
    public double maxTime()
    {
        return MAXIMUM_TIME_UNDERWATER;
    }
    
    public double getMeasure()
    {
        return height;
    }
}
