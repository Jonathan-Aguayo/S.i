public class Dog
{
    //Instance vars
    private String name;
    private int age;

    //constructor
    public Dog (String Name, int Age)
    {
        name = Name;
        age = Age;
    }

    //methods
    public String getName()
    {
        return name;
    }

    public void setName(String newName)
    {
        name=newName;
    }

    public int getAge()
    {
        return age;
    }

    public int getDogYears()
    {
        return age*5;
    }
}
