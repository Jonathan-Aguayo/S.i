
public class Dog
{
    //Instance vars
    private String name;
    private int height;

    //constructor
    public Dog (String Name,int Height)
    {
        this.name = Name;
        this.height=Height;
    }

    //methods
    public String getName()
    {
        return name;
    }

    public void setName(String newName)
    {
        name=newName;
    }
}
