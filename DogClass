
public class Dog
{
    //Instance vars
    private String name;
    private double height;

    //constructor
    public Dog (String Name,double Height)
    {
        this.name = Name;
        this.height=Height;
    }

    //Methods
    public String getName()
    {
        return name;
    }

    public void setName(String newName)
    {
        name=newName;
    }
    
    public double height
    {
        return height;
    }
    public String toString()
    {
        return this.name + "-->" + this.height;
    }

}
